{
    "manifest_version": 3,
    "name": "Skill Auto Selector",
    "version": "1.0",
    "description": "Automatically selects skills from the dropdown.",
    "permissions": ["scripting", "activeTab"],
    "host_permissions": ["<all_urls>"],
    "action": {
      "default_popup": "popup.html"
    }
  }
  